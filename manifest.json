{
  "manifest_version": 1,
  "name": "WriteBack Guru",
  "description": "A Tableau extension for writeback capabilities.",
  "version": "1.0.0",
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "storage",
    "activeTab",
    "declarativeContent",
    "https://ajax.googleapis.com/",
    "https://maxcdn.bootstrapcdn.com/",
    "https://tableau.github.io/",
    "http://localhost:8080/"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icon.png"
    }
  }
}
